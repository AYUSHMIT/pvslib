std_syscall_examples : THEORY
BEGIN

  IMPORTING std_syscall

  ex1 : void =
    LET (status,mssg) = system_call("date") IN
    print(date)

  ex2 : [nat,string] =  % Status is 0 if command succeds
    system_call("ls")

  ex2a : [nat,string] = % Since file doesn't exist, status is not zero
    system_call("ls non-existent-file.txt")

END std_syscall_examples
