time_examples : THEORY
BEGIN

  IMPORTING time_fun,
            time_exec,
	    is_prime

  ex1 : void =
    tic & % Starts timer
    LET proc = is_prime?(37139213),  % This takes a few seconds to compute
        (realtime,runtime) = toc IN  % Times are in seconds
    printf("Real Time: ~a. Run Time: ~a", {| hms(realtime), hms(runtime) |})

  ex2 : bool = % Time the execution is_prime?(...)
    time_exec(is_prime?(37139213))

END time_examples
