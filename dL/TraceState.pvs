TraceState : DATATYPE
BEGIN
  IMPORTING hp_def,
            hp_expr

  STATE(state : Environment) : state?
  INF_DIFF(s0:Environment, inf_behavior:[(hp(0)) -> Environment]) : inf_diff?
  STATE_DIFF(D:{D:(dd?) | EXISTS(b:posreal): D = closed_interval(0,b)}
            , s0:Environment, behavior:[(D) -> Environment]) : state_diff?
  ERROR : error?

END TraceState
